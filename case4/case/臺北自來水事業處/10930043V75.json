61c9\u6309\u671f\uff08\u4ee5\u6bcf3\u500b\u6708\u70ba1\u671f\uff09\u7e73\u4ea4\uff0c\u9664\u7b2c1\u671f\u4f7f\u7528\u8cbb\u53ca\u71df\u696d\u7a05\u81f3\u9072\u61c9\u65bc\u571f\u5730\u9ede\u4ea4\u5b8c\u6210\u6b21\u65e5\u524d\u7e73\u4ea4\u5916\uff0c\u5176\u9918\u5404\u671f\u4f7f\u7528\u8cbb\u53ca\u71df\u696d\u7a05\uff0c\u61c9\u65bc\u6bcf\u5e741\u6708\u30014\u6708\u30017\u6708\u53ca10\u6708\u4e4b\u7576\u67085\u65e5\u524d\u9810\u5148\u7e73\u4ea4\u4e4b\uff0c\u9918\u8a73\u884c\u653f\u5951\u7d04\u3002 \u4e5d\u3001\u5c65\u7d04\u4fdd\u8b49\u91d1\uff1a\u5f97\u6a19\u4eba\u61c9\u65bc\u6c7a\u6a19\u5f8c\u7c3d\u7d04\u524d\uff0c\u7e73\u7d0d\u76f8\u75762\u500b\u6708\u4f7f\u7528\u8cbb\u53ca\u71df\u696d\u7a05\u91d1\u984d\u4e4b\u4fdd\u8b49\u91d1\u4e88\u672c\u8655\u3002 \u5341\u3001\u7279\u6b8a\u898f\u5b9a\uff1a\u8a73\u672c\u6848\u88dc\u5145\u8aaa\u660e\u53ca\u884c\u653f\u5951\u7d04\u3002 \u5341\u4e00\u3001\u5951\u7d04\u6587\u4ef6\u4f9d\u6a5f\u95dc\u898f\u5b9a\u683c\u5f0f\u5f71\u5370\u53ca\u88dd\u8a02\uff0c\u6240\u9700\u8cbb\u7528\u7531\u5f97\u6a19\u4eba\u8ca0\u64d4\u3002 \u5341\u4e8c\u3001\u61c9\u9644\u8b49\u660e\u6587\u4ef6\uff1a\u81ea\u7136\u4eba\uff081.\u570b\u6c11\u8eab\u5206\u8b49\u6b63\u53cd\u9762\u5f71\u672c2.\u7d0d\u7a05\u8b49\u660e\u8cc7\u65993.\u975e\u62d2\u7d55\u5f80\u4f86\u6236\u53ca\u6700\u8fd1\u4e09\u5e74\u5167\u7121\u9000\u7968\u8a18\u9304\u4e4b\u91d1\u878d\u6a5f\u69cb\u8b49\u660e\u6587\u4ef64.\u62bc\u6a19\u91d1\u7e73\u7d0d\u6191\u64da\uff09\u3002\u516c\u53f8\u3001\u884c\u865f\uff081.\u4e3b\u7ba1\u6a5f\u95dc\u6838\u51c6\u8a2d\u7acb\u4e4b\u8b49\u4ef6\u5f71\u672c2.\u7d0d\u7a05\u8b49\u660e\u8cc7\u65993.\u975e\u62d2\u7d55\u5f80\u4f86\u6236\u53ca\u6700\u8fd1\u4e09\u5e74\u5167\u7121\u9000\u7968\u8a18\u9304\u4e4b\u91d1\u878d\u6a5f\u69cb\u8b49\u660e\u6587\u4ef64.\u62bc\u6a19\u91d1\u7e73\u7d0d\u6191\u64da\uff09\u3002\u975e\u5c6c\u71df\u5229\u4e8b\u696d\u4e4b\u6cd5\u4eba\uff081.\u6cd5\u4eba\u767b\u8a18\u8b49\u660e\u66f8\u5f71\u672c2.\u7ae0\u7a0b3.\u7d0d\u7a05\u8b49\u660e\u8cc7\u65994.\u975e\u62d2\u7d55\u5f80\u4f86\u6236\u53ca\u6700\u8fd1\u4e09\u5e74\u5167\u7121\u9000\u7968\u8a18\u9304\u4e4b\u91d1\u878d\u6a5f\u69cb\u8b49\u660e\u6587\u4ef65.\u62bc\u6a19\u91d1\u7e73\u7d0d\u6191\u64da\uff09\u3002\u9918\u8a73\u6295\u6a19\u9808\u77e5\u3002 \u5341\u4e09\u3001\u6295\u6a19\u5c01\u5957\uff0c\u61c9\u8cfc\u7528\u672c\u8655\u6240\u63d0\u4f9b\uff1b\u672a\u4f9d\u898f\u5b9a\u4f7f\u7528\u8005\u70ba\u7121\u6548\u6a19\u3002 \u5341\u56db\u3001\u672c\u6848\u8fa6\u7406\u516c\u958b\u62db\u6a19\uff0c\u5408\u683c\u6295\u6a19\u5bb6\u6578\u57281\u5bb6\u4ee5\u4e0a\u6642\uff0c\u5373\u958b\u6a19\u5be9\u6a19\u3002 \u5341\u4e94\u3001\u672c\u6848\u622a\u6b62\u6536\u4ef6\u65e5\u6216\u958b\u6a19\u65e5\u70ba\u8fa6\u516c\u65e5\uff0c\u800c\u8a72\u65e5\u56e0\u6545\u505c\u6b62\u8fa6\u516c\uff0c\u4ee5\u6b21\u4e00\u8fa6\u516c\u65e5\u4e4b\u540c\u4e00\u622a\u6b62\u6536\u4ef6\u6216\u958b\u6a19\u6642\u9593\u4ee3\u4e4b\u3002 \u5341\u516d\u3001\u7591\u7fa9\u3001\u7570\u8b70\u53d7\u7406\u7a97\u53e3\uff1a\u81fa\u5317\u81ea\u4f86\u6c34\u4e8b\u696d\u8655\uff0c\u96fb\u8a71\uff1a02-87335796\u3001\u50b3\u771f\uff1a02-87335794\u3001\u5730\u5740\uff1a\u81fa\u5317\u5e02\u5927\u5b89\u5340\u9577\u8208\u8857131\u865f\u3002 \u5341\u4e03\u3001\u5951\u7d04\u7c3d\u8a02\u65e5\u4ee5\u6c7a\u6a19\u5f8c\u7b2c15\u65e5\u8996\u70ba\u96d9\u65b9\u5408\u610f\u65bc\u5951\u7d04\u6587\u66f8\u6240\u8f09\u8a02\u7d04\u65e5\u3002 \u5341\u516b\u3001\u672c\u6848\u9810\u5b9a\u6c7a\u6a19\u65e5\u540c\u958b\u6a19\u65e5\u3002","fetched_at":"2020-07-09T23:26:32+08:00"},"unit_name":"\u81fa\u5317\u81ea\u4f86\u6c34\u4e8b\u696d\u8655","unit_api_url":"http:\/\/pcc.g0v.ronny.tw\/api\/listbyunit?unit_id=3.79.27","tender_api_url":"http:\/\/pcc.g0v.ronny.tw\/api\/tender?unit_id=3.79.27&job_number=10930043V75","unit_url":"\/index\/unit\/3.79.27","url":"\/index\/case\/3.79.27\/10930043V75\/20200709\/FAR-1-50049316"}]}